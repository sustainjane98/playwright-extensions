{
  "name": "docs/playwright-extensions",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "docs/playwright-extensions/src",
  "projectType": "application",
  "targets": {
    "docusaurus": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "docs/playwright-extensions",
        "command": "npx docusaurus"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "docs/playwright-extensions",
        "command": "npx docusaurus build --out-dir ../../dist/docs/playwright-extensions"
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "docs/playwright-extensions",
        "command": "npx docusaurus start"
      },
      "dependsOn": ["build"]
    },
    "clear": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "docs/playwright-extensions",
        "command": "npx docusaurus clear --siteDir ../../dist/docs/playwright-extensions"
      },
      "dependsOn": ["build"]
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "docs/playwright-extensions",
        "command": "npx docusaurus deploy --out-dir ../../dist/docs/playwright-extensions"
      },
      "dependsOn": ["build"]
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "docs/playwright-extensions",
        "command": "npx docusaurus serve --dir ../../dist/docs/playwright-extensions/build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/playwright-extensions/**/*.ts",
          "libs/playwright-extensions/package.json"
        ]
      }
    }
  },
  "tags": []
}
